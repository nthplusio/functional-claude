{
  "description": "Session insights hooks - quick data availability check on session start",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/session-insights-session-start.js",
            "timeout": 10
          }
        ]
      }
    ]
  }
}