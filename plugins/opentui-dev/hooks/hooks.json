{
  "description": "opentui-dev hooks - automatic cache refresh on session start",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node \"${CLAUDE_PLUGIN_ROOT}/hooks/opentui-session-start.js\"",
            "timeout": 30
          }
        ]
      }
    ]
  }
}
